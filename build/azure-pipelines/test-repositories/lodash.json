{
	"name": "lodash",
	"repository": "https://github.com/lodash/lodash.git",
	"init": [
		{ "command": "npm", "args": ["install"] }
	],
	"tests": [
		{
			"config": {
				"project": {
						"compilerOptions": {
								"rootDir": "./"
						},
						"include": [
								"*.js",
								"test/*.js"
						]
				},
				"out": "lodash.lsif",
				"package": "./package.json",
				"source": "./package.json"
			}
		}
	]
}